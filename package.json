{
  "name": "precise",
  "description": "Precision timer for nanosecond differences",
  "version": "2.0.0",
  "homepage": "http://avoidwork.github.io/precise",
  "author": "Jason Mulligan <jason.mulligan@avoidwork.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/avoidwork/precise.git"
  },
  "bugs": {
    "url": "https://github.com/avoidwork/precise/issues"
  },
  "license": "BSD-3-Clause",
  "source": "src/precise.js",
  "main": "dist/precise.cjs",
  "module": "dist/precise.esm.js",
  "type": "module",
  "sourceType": "module",
  "engineStrict": true,
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    "build": "npm run lint && rm -rf dist/* && npm run rollup",
