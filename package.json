    "mocha": "mocha test/*.js",
    "test": "npm run lint && npm run mocha"
  },
  "devDependencies": {
    "auto-changelog": "^2.4.0",
    "eslint": "^8.23.0",
    "mocha": "^10.0.0",
    "rollup": "^2.79.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "keywords": [
    "cpu",
    "precise",
    "precision",
    "nanosecond",
    "timer",
    "time",
    "difference",
    "elasped"
